<edit-config xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
    <target>
      <running/>
    </target>
    <config>
      <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
        <logging>
          <trap>
            <severity>{{ severity }}</severity>
          </trap>
          <hostip>{{ hostip }}</hostip>
        </logging>
      </native>
    </config>
  </edit-config>